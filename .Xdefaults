! ~/.Xdefaults !

!!! rxvt-unicode

!! Colors
*cursorColor: #d3d7cf
URxvt*background: #2e3436
URxvt*foreground: #eeeeec
Rxvt*background: #2e3436
Rxvt*foreground: #eeeeec
! Black
*color0: #2e2326
*color8: #555753
! Red
*color1: #CC0000
*color9: #ef2929
! Green
*color2: #73d216
*color10: #8ae234
! Yellow
*color3: #edd400
*color11: #fce94f
! Blue
*color4: #3465a4
*color12: #729fcf
! Magenta
*color5: #75507b
*color13: #ad7fa8
! Cyan
*color6: #00AAAA
*color14: #55FFFF
! White
*color7: #AAAAAA
*color15: #FFFFFF

! Tabs
URxvt.tabbed.tabbar-fg: 7
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 7

!! UI
Rxvt*scrollstyle: plain
Rxvt*scrollBar_right: true
Rxvt*font: Droid Sans Mono-20

!! Extensions
URxvt*perl-ext-common: default,matcher
URxvt*urlLauncher: firefox

!! Misc.
URxvt*pointerBlank: True

